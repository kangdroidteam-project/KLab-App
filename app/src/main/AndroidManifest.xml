<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kangdroid.k_labapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".view.GardenReservationActivity"></activity>
        <activity android:name=".view.SplashActivity">
        </activity>
        <activity android:name=".view.MainActivity" />
        <activity android:name=".view.RegisterActivity" />
        <activity android:name=".view.MyClassListActivity"/>
        <activity android:name=".view.ClassDetailActivity" />
        <activity android:name=".view.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.TabActivity" />
        <activity android:name=".view.ClassMakeActivity"/>
        <activity android:name=".view.MyHostClassActivity"/>
        <activity android:name=".view.MatchingActivity"/>
        <activity android:name=".view.MatchingDetailActivity"/>

    </application>

</manifest>